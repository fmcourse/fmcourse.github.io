{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Generate png from dot",
            "type": "shell",
            "command": "dot",
            "args": ["-Tpng", "${file}", "-o", "${fileDirname}/${fileBasenameNoExtension}.png"]    
        }
    ]
}